global:
  resolve_timeout: 5m

route:
  group_by: ['alertname']
  group_wait: 10s
  group_interval: 10s
  repeat_interval: 1h
  receiver: 'email'

receivers:
- name: 'email'
  email_configs:
  - to: email
    from: email
    smarthost: smtp.gmail.com:587
    auth_username: "email"
    auth_identity: "email"
    auth_password: ""